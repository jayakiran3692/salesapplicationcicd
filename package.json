{
  "name": "salesapplication",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.2.0",
    "@sap-cloud-sdk/resilience": "^4.2.0",
    "@sap/approuter": "^20.8.6",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-salesapplication": "cds watch --open salesappl.salesapplication/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "Northwind": {
        "kind": "odata",
        "model": "srv/external/northwind",
        "credentials": {
          "url": "https://services.odata.org/V4/Northwind/Northwind.svc/"
        }
      },
      "[production]": {
        "auth": "xsuaa",
        "db": "hana"
      },
      "auth": {
        "kind": "mocked",
        "users": {
          "viewer1": {
            "password": "viewer123",
            "roles": [
              "Viewer"
            ]
          },
          "admin1": {
            "password": "admin123",
            "roles": [
              "Admin"
            ]
          }
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/salesapplication",
    "app/salesapplication"
  ]
}
